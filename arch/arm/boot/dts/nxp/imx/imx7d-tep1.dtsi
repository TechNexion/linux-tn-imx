			fsl,pins = <
				MX7D_PAD_GPIO1_IO10__PWM3_OUT		0x0
			>;
		};

		pinctrl_hog_1: gpio_ctrlgrp {
			fsl,pins = <
				MX7D_PAD_I2C4_SDA__GPIO4_IO15		0x15	/* fec power */
			>;
