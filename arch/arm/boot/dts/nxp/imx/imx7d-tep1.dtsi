		pinctrl_ft5606_irq: ft5606-touch-irq {
			fsl,pins = <
				MX7D_PAD_EPDC_DATA11__GPIO2_IO11	0x59	/* touch irq */
			>;
		};

		pinctrl_ft5606_reset: ft5606-touch-reset {
			fsl,pins = <
				MX7D_PAD_EPDC_DATA10__GPIO2_IO10	0x59	/* touch rest */
			>;
