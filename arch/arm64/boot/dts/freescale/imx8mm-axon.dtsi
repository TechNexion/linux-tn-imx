			>;
		};

		pinctrl_enet_pwr: enetpwrgrp {
			fsl,pins = <
				MX8MM_IOMUXC_NAND_CE0_B_GPIO3_IO1		0x11
			>;
		};

		pinctrl_i2c1: i2c1grp {
			fsl,pins = <
