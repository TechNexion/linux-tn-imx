#include "imx8mm-xore-wizard.dts"

/ {
        backlight_exp_mipi: backlight_exp_mipi {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 50000 0>;
		brightness-levels = <0 36 72 108 144 180 216 255>;
		default-brightness-level = <6>;
		power-supply = <&reg_backlight_pwr>;
                status = "okay";
        };
};

&mipi_dsi {
       status = "okay";

        panel@0 {
                compatible = "auo,g080uan01";
                reg = <0>;
                pinctrl-0 = <&pinctrl_mipi_dsi_rst>;
                enable-gpios = <&gpio1 10 GPIO_ACTIVE_HIGH>;
                backlight = <&backlight_exp_mipi>;
                status = "okay";
        };
};

&i2c3 {
        polytouch: edt-ft5x06@38 {
                compatible = "edt,edt-ft5x06";
                reg = <0x38>;
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_touch_irq>;
                interrupt-parent = <&gpio3>;
                interrupts = <22 IRQ_TYPE_EDGE_FALLING>;
                reset-gpios = <&pca9555_1 5 GPIO_ACTIVE_LOW>;
                touchscreen-size-x = <1200>;
                touchscreen-size-y = <1920>;
        };
};















